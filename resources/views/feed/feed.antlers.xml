{{ xml_header }}
<rss version="2.0">
   <channel>
      <title>{{ config:app:name }} Journal</title>
      <link>{{ site:url }}</link>
      <description>Readeable bites about web development.</description>
      <language>en-us</language>
      <lastBuildDate>Tue, 10 Jun 2003 09:41:01 GMT</lastBuildDate>
      <webMaster>info@1902.studio</webMaster>
      {{ collection:journal sort="date:desc" limit="50" }}
        <item>
            <title>{{ title }}</title>
            <link>{{ url }}</link>
            <description><![CDATA[ ]]></description>
            <pubDate>{{ date iso_format="ddd, DD MMM GGGG HH:mm:ss UTC" }}</pubDate>
        </item>
      {{ /collection:journal }}
   </channel>
</rss>
